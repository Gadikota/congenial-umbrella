<tr>
  <td> <%= product.name %> </td>
  <td> <%= product.price %> </td>
  <td> <%= product.category.title %> </td>
  <td> <%= product.created_at %> </td>
  <td> <%= link_to "Edit", edit_product_path(product) %>
    <br/>
    <%= link_to "Detail Page", product_path(product) %>
    <br/ >
    <%= link_to "Delete ?", product_path(product), method: :delete, data: { confirm: "Are you sure ?"} %>
   </td>
</tr>
<table>
  <thead>
    <th> Images </th>
  </thead>
  <tbody>
    <%= render partial: 'products/images/image', collection: product.images %>
  </tbody>
</table>